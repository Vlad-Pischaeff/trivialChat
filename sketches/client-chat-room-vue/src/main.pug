extends elements/layout 

block content
  section.cp#App(v-cloak)
    .cp_header
      picture.cp_header-avatar
        img.cp_header-avatarimg(:src="avatar ? avatar : 'data:image/png;base64,'")
        div.cp_header-avatarbg
      .cp_header-card
        div.cp_header-card-1 {{ organisation ? organisation : 'Lorem ipsum dolor sit amet, consectetur mollit'}}
        div.cp_header-card-2 {{ description ? description : 'I am manager. Please ask Your question... Lorem ipsum dolor sit amet, consectetur mollit...'}}
    .cp_body
      .chat_field
        .chat_field-message(v-if="greeting !== ''" data-align="to")
          div.chat_field-messageto
            p.msg-data {{ new Date().toLocaleString() }}
            p.msg-text {{ greeting }}
        .chat_field-message(v-for='(msg, idx) in messages' :data-align="msg.from ? 'from' : 'to'" ref="msg")
          div(:class="msg.from ? 'chat_field-messagefrom' : 'chat_field-messageto'")
            p.msg-data {{ new Date(msg.date).toLocaleString() }}
            p.msg-text {{ msg.msg }}, {{ idx + 1 }}
    .cp_footer
      .chat_input
        input.chat_input-text(
                  name="question" 
                  type="text" 
                  :placeholder='placeholderStr'
                  :value='inputVal'
                  @input='inputHandler'
                  @keypress.enter ='addMessage'
                  required 
                  autofocus ) 
        img.chat_input-icon2(src="data:image/png;base64," alt='OK' @click='addMessage')
